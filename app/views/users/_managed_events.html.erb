<table class="table">
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Location</th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% events.each do |event| %>
      <tr>
        <td><%= event.title %></td>
        <td><%= event.description %></td>
        <td><%= event.location %></td>
        <td><%= link_to "view event", event_path(event), class: "btn btn-primary" %></td>
        <td><%= link_to "edit", edit_event_path(event), class: "btn btn-default" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
